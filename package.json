{
  "name": "next-boiler-plate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./env/.env.local next dev",
    "dev:docker-local": "docker-compose  --env-file ./env/.env.local up --build",
    "down:docker-local": "docker-compose --env-file ./env/.env.local down -v",
    "build": "next build",
    "prestart": "npm run build",
    "start": "pm2 list && pm2-runtime start ecosystem.config.js",
    "exec": "docker exec -it admin-nextjs-frontend /bin/sh",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "json:i18n": "node ./src/i18n/xlsx/xlsx_to_json.js",
    "xlsx:i18n": "node ./src/i18n/xlsx/json_to_xlsx.js"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "env-cmd": "^10.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jwt-decode": "^4.0.0",
    "next": "14.1.0",
    "next-client-cookies": "^1.1.1",
    "react": "^18",
    "react-dom": "^18",
    "react-textarea-autosize": "^8.5.3",
    "xlsx": "^0.18.5",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.2.20",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/preview-api": "^7.6.17",
    "@storybook/react": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "storybook": "^8.0.0",
    "storybook-react-i18next": "^3.0.1",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
